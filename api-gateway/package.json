    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jsdoc": "^3.6.4",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
